{
    "name": "Insert into db",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":["storage", "activeTab" , "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js" , "type":"module"
        
      },
      "minimum_chrome_version": "92",
